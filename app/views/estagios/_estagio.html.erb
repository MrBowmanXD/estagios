<div id="<%= dom_id estagio %>">
  <p class="card-header subtitle center" style="flex-direction: row;">
    <span class="bold">Titulo:</span>
    <%= estagio.titulo %>
  </p>

  <div class="card-content">
    <p style="text-align: center;">
      <strong>Descricao:</strong>
      <%= estagio.descricao %>
    </p>

    <p style="text-align: center;">
      <strong>Localidade:</strong>
      <%= estagio.localidade %>
    </p>
  </div>


</div>
